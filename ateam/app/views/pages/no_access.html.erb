<%=render "pages/index/landing/signed_in_content"%>

<div class = "modal-overlay" id = "no-access-overlay"></div>
<div class = "modal modal-positioning" id = "no-access-modal">
  <div id = "signup-initializer" class = "modal-container">
    <div class = "GM-modal-wrapper">
      <div class = "modal-header-wrapper">
        <div class = "modal-header" id = "create-account-header">No Access</div>
      </div>
      <div class = "modal-inner-content" id = "signup-modal-content">
        <div class = "modal-body ">
          
          <div class = "modal-message">It appears your coach is no longer subscribed to ATeam. Contact your coach to reactivate the team. If you believe a mistake has been made, contact our support team at support@ateamsports.awsapps.com.</div>

        </div>
      </div>
      <div class = "modal-footing">
      	<button class = "modal-cancel-button" id = "no-access-cancel" onclick = "close_no_access_modal()">Cancel</div>
      </div>
      
    </div>
  </div>
</div>


<script type="text/javascript">
	function close_no_access_modal(){
		$("#no-access-modal").hide()
		$("#no-access-overlay").hide()
	}
</script>